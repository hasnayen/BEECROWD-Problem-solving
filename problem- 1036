import math
A,B,C = input().split()

A = float(A)
B = float(B)
C = float(C)

r = B*B-4*A*C
if r < 0 or A == 0:
       print("Impossivel calcular")
else :
        R1 = (-B + math.sqrt(r))/(2*A)
        R2 = (-B - math.sqrt(r))/(2*A)
        print(f'R1 = {round(R1,5)}')
        print(f'R2 = {round(R2,5)}')



    
